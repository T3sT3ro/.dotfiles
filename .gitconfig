	email = 5300963+T3sT3ro@users.noreply.github.com
	name = Tooster
	signingkey = ssh-rsa  tooster@toosterLap
[alias]
	dog = log --decorate --oneline --graph
    adog = dog --all
    dog2 = log --graph --pretty=format:'%C(auto)%<|(16)%h%Creset %C(dim green)%>|(18)%cr%Creset\t - %C(dim)%s %C(italic yellow)<%an>%Creset %C(auto)%d%Creset' --abbrev-commit
    adog2 = dog2 --all
    dog3 = log --all --graph --pretty=format:'%C(auto)%<|(20)%h%Creset %C(bold cyan)%cd%Creset %C(dim green)(%cr)%Creset %C(dim italic yellow)%<|(74)~%cn%Creset %C(auto)%d%Creset %>|(80)%m %C(dim)%s' --abbrev-commit --date=format-local:'%d-%m-%Y  %H:%M'
    adog3 = dog3 --all
	branch-name = "!git rev-parse --abbrev-ref HEAD"
	seppuku = "!f(){ BNAME=$(git branch-name); git reset --hard origin/${BNAME}; }; f;"
    publish = "!git push -u origin $(git branch-name)"
    fixup="commit --amend --no-edit"
    reword="commit --amend -m"
    show-tracked="ls-tree -r master --name-only"
    pull-all="pull --recurse-submodules"
[merge]
	tool = vscode
[mergetool "vscode"]
	cmd = code --wait --merge $REMOTE $LOCAL $BASE $MERGED
[mergetool "unity"]
	trustExitCode = false
	cmd = /home/tooster/Unity/Hub/Editor/2018.4.8f1/Editor/Data/Tools/UnityYAMLMerge merge -p "$BASE" "$REMOTE" "$LOCAL" "$MERGED"
[mergetool "meld"]
	trustExitCode = false
	cmd = meld "$LOCAL" "$BASE" "$REMOTE" --output "$MERGED"
[diff]
	tool = vscode
[difftool "vscode"]
	cmd = code --wait --diff $LOCAL $REMOTE
	

[color]
	ui = auto
[core]
	editor = vim
	autocrlf = input
#    pager = "less -iXFR"
[commit]
	gpgsign = true
[gpg]
	format = ssh
[init]
	defaultBranch = master
